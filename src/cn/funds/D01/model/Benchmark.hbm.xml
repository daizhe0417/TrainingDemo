<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.funds.D01.model.Benchmark" table="benchmark" schema="funds">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="native"></generator>
        </id>
        <property name="benchmarkName">
            <column name="benchmarkName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="deltag">
            <column name="deltag" sql-type="char(1)" length="1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>