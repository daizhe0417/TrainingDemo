<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.funds.D01.model.DingxingTable" table="dingxingTable" schema="funds">
        <id name="id">
            <column name="id" sql-type="int(11)"/><generator class="native"></generator>
        </id>
        <property name="companyId">
            <column name="companyId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="itemName">
            <column name="itemName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="itemScore">
            <column name="itemScore" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="scoreName">
            <column name="scoreName" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>