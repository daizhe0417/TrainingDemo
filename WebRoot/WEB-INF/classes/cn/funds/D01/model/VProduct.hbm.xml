<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.funds.D01.model.VProduct" table="v_product" schema="funds">
        <id name="id">
            <column name="id" sql-type="int(20)"/>
            <generator class="native"></generator>
        </id>
        <property name="productName">
            <column name="ProductName" sql-type="char(50)" length="50"/>
        </property>
        <property name="managementId">
            <column name="ManagementID" sql-type="int(8)"/>
        </property>
        <property name="companyId">
            <column name="CompanyID" sql-type="int(8)"/>
        </property>
        <property name="largeClass">
            <column name="LargeClass" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="middleClass">
            <column name="MiddleClass" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="littleClass">
            <column name="LittleClass" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="foundDate">
            <column name="FoundDate" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="scale">
            <column name="Scale" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="cutOffDate">
            <column name="cutOffDate" sql-type="text" not-null="true"/>
        </property>
        <property name="annualIncome">
            <column name="AnnualIncome" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="annualRisk">
            <column name="AnnualRisk" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="sharpRatio">
            <column name="SharpRatio" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="maxRetracement">
            <column name="MaxRetracement" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="deltag">
            <column name="deltag" sql-type="char(1)" length="1" not-null="true"/>
        </property>
        <property name="productCode">
            <column name="productCode" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="managementName">
            <column name="ManagementName" sql-type="char(50)" length="50" not-null="true"/>
        </property>
        <property name="companyName">
            <column name="CompanyName" sql-type="char(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>