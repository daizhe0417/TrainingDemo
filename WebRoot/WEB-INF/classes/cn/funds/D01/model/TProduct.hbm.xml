<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.funds.D01.model.TProduct" table="t_product" schema="funds">
        <id name="id">
            <column name="id" sql-type="int(20)"/><generator class="native"></generator>
        </id>
        <property name="productName">
            <column name="ProductName" sql-type="char(50)" length="50"/>
        </property>
        <property name="managementId">
            <column name="ManagementID" sql-type="int(8)"/>
        </property>
        <property name="companyId">
            <column name="CompanyID" sql-type="int(8)"/>
        </property>
        <property name="largeClass">
            <column name="LargeClass" sql-type="char(20)" length="20"/>
        </property>
        <property name="middleClass">
            <column name="MiddleClass" sql-type="char(20)" length="20"/>
        </property>
        <property name="littleClass">
            <column name="LittleClass" sql-type="char(20)" length="20"/>
        </property>
        <property name="foundDate">
            <column name="FoundDate" sql-type="varchar(255)"/>
        </property>
        <property name="scale">
            <column name="Scale" sql-type="char(50)" length="50"/>
        </property>
        <property name="cutOffDate">
            <column name="CutOffDate" sql-type="varchar(255)"/>
        </property>
        <property name="netValue">
            <column name="NetValue" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="annualIncome">
            <column name="AnnualIncome" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="sharpRatio">
            <column name="SharpRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="maxRetracement">
            <column name="MaxRetracement" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="informationRatio">
            <column name="InformationRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="treynorRatio">
            <column name="TreynorRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="sortinoRatio">
            <column name="SortinoRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="implementBeta">
            <column name="ImplementBeta" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="mSquare">
            <column name="MSquare" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="omegaRatio">
            <column name="OmegaRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="kurtosis">
            <column name="Kurtosis" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="skewness">
            <column name="Skewness" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="jasenRatio">
            <column name="JasenRatio" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="score">
            <column name="Score" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="dwjz">
            <column name="dwjz" sql-type="decimal(10,3)" precision="10" scale="3"/>
        </property>
        <property name="deltag">
            <column name="deltag" sql-type="char(1)" length="1"/>
        </property>
        <property name="productCode">
            <column name="productCode" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>