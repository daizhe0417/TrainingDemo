<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.funds.D02.model.Combine" table="combine" schema="funds">
        <id name="id">
            <column name="id" sql-type="int(11)"/><generator class="native"></generator>
        </id>
        <property name="comName">
            <column name="comName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="clrq">
            <column name="clrq" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="cjrq">
            <column name="cjrq" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="comment">
            <column name="comment" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="deltag">
            <column name="deltag" sql-type="char(1)" length="1" not-null="true"/>
        </property>
        <property name="cpsl">
            <column name="cpsl" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>