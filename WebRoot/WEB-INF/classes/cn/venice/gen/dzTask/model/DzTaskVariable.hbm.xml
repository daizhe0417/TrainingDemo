<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.venice.gen.dzTask.model.DzTaskVariable" table="dz_task_variable" schema="trainingDemo">
        <id name="id">
            <column name="id" sql-type="int(11)"/><generator class="native"></generator>
        </id>
        <property name="taskId">
            <column name="taskId" sql-type="int(255)"/>
        </property>
        <property name="variableName">
            <column name="variableName" sql-type="varchar(255)"/>
        </property>
        <property name="varValue">
            <column name="varValue" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>